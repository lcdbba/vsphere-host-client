   <!-- Type filter -->
   <extension id="vsphere.core.typeFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.TypeFilter"/>
         <returnsInstantly>true</returnsInstantly>

         <!-- Context is the data the filter expects to get from the extension host -->
         <contextProperties>
            <!-- The server object ref types is matched against the objectType provided
            by the extension. If extension can support all the types in the current
            context then it is marked as applicable by the filter -->
            <com.vmware.extensionfw.PropertyInfo>
               <name>contextObjects</name>
               <type>Array</type>
            </com.vmware.extensionfw.PropertyInfo>
         </contextProperties>

         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- If the object type is specified as *, then it means extension is
               applicable to all kinds of object -->
               <name>objectType</name>
               <!-- comma separated list of object types -->
               <type>String</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Exclude filter -->
   <extension id="vsphere.core.excludeFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.ExcludeFilter"/>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Privilege filter -->
   <extension id="vsphere.core.privilegeFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.PrivilegeFilter"/>

         <!-- Context is the data the filter expects to get from the extension host -->
         <contextProperties>
            <!-- The server object ref types is matched against the objectType provided
            by the extension. If extension can support all the types in the current
            context then it is marked as applicable by the filter If the contextObjects
            are not specified, then privilege checks are performed on the root folders.-->
            <com.vmware.extensionfw.PropertyInfo>
               <name>contextObjects</name>
               <type>Array</type>
               <optional>true</optional>
            </com.vmware.extensionfw.PropertyInfo>
         </contextProperties>

         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- An array of privilege ids that need to be granted to a user to access
               ths extension -->
               <name>privilege</name>
               <!-- comma separated list of privilege IDs-->
               <type>String</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Custom Attribute filter
   Used to filter out any extensions that do not have the specified custom attribute
   key-value pair defined.
   -->
   <extension id="vsphere.core.customAttributeFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.CustomAttributeFilter"/>

         <!-- Context is the data the filter expects to get from the extension host -->
         <contextProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <name>contextObjects</name>
               <type>Array</type> <!-- An array of com.vmware.core.model.ServerObjectRef -->
               <!-- Currently the filter is applicable only to an object not an array -->
            </com.vmware.extensionfw.PropertyInfo>
         </contextProperties>

         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- Name of the custom attribute -->
               <name>attributeKey</name>
               <type>String</type>
            </com.vmware.extensionfw.PropertyInfo>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- Value of the custom attribute -->
               <name>attributeValue</name>
               <type>String</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- UserSessionEnvPropertyFilter filter -->
   <extension id="vsphere.core.userSessionEnvPropertyFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
