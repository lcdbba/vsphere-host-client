   <!-- Disabled Operation filter -->
   <extension id="vsphere.core.disabledOperationFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass
            className="com.vmware.vsphere.client.filters.DisabledOperationFilter"/>

         <!-- Context is the data the filter expects to get from the extension host -->
         <contextProperties>
            <!-- The server object ref types is matched against the objectType provided
            by the extension. If extension can support all the types in the current
            context then it is marked as applicable by the filter -->
            <com.vmware.extensionfw.PropertyInfo>
               <name>contextObjects</name>
               <type>Array</type>
            </com.vmware.extensionfw.PropertyInfo>
         </contextProperties>

         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- Operations that need to be available for the extension
               to be valid -->
               <name>operationId</name>
               <!-- comma separated list of operation ids -->
               <type>String</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Server Guid filter -->
   <extension id="vsphere.core.vcServerGuidFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.VcServerGuidFilter"/>
         <returnsInstantly>true</returnsInstantly>

         <!-- The properties that the filter expects to get from the extension host. -->
         <contextProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <name>contextObjects</name>
               <type>Array</type> <!-- array of com.vmware.core.model.ServerObjectRef -->
               <!-- optional is true so that global extensions (applications etc)
                  pass the filter. -->
               <optional>true</optional>
            </com.vmware.extensionfw.PropertyInfo>
         </contextProperties>

         <!-- The properties required by the filter and specified in
            the extension metadata. -->
         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <name>vcServerGuids</name>
               <type>Array</type> <!-- array of guid String -->
               <!-- optional is true so that non-vc extensions pass the filter. -->
               <optional>true</optional>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Extension Object Content Filter-->
   <extension id="vsphere.core.extensionContentFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass
            className="com.vmware.vsphere.client.filters.ExtensionObjectFilter"/>
         <returnsInstantly>true</returnsInstantly>

         <!-- Context is the data the filter expects to get from the extension host -->
         <contextProperties>
            <!-- The propertyConstraints are used to compare against the content of
            the extension object XML.
            propertyName : Must be a valid xpath in the XML object. Note that the
               last fragment in the path can be a node or name of an attribute. Also,
               "." or "" implies text of rootNode.
            comparableValue : Can be CSV of values to compare against.
            comparator : Currently supports EQUALS and EQUALS_ANY_OF operators.  -->
            <com.vmware.extensionfw.PropertyInfo>
               <name>propertyConstraints</name>
               <type>Array</type> <!-- Array of com.vmware.data.query.PropertyConstraint -->
            </com.vmware.extensionfw.PropertyInfo>
         </contextProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Object Property filter-->
   <extension id="vsphere.core.objectPropertyFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.ObjectPropertyFilter"/>

         <!-- Context is the data the filter expects to get from the extension host -->
         <contextProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <name>contextObjects</name>
               <type>Array</type> <!-- An array of com.vmware.core.model.ServerObjectRef -->
            </com.vmware.extensionfw.PropertyInfo>
         </contextProperties>
         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- 1. Expected that all the properties specified in the propertyConditions
                    are properties that can be retrieved using the DataService on the
                    object supplied as context. Apart from these the filter additionally
                    supports 'parent.type' property.
                    2. The supported comparison operators are EQUALS, CONTAINS and EQUALS_ANY_OF.
                    3. The supported conjoiner operators are AND, OR.
                    -->
               <name>propertyConditions</name>
               <type>com.vmware.data.query.CompositeConstraint</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Filters out the extensions that have no extensions specified for their
      hosted extension point. This can be used, for example, to filter out empty tabs.
      The filter operates on extensions that specify true for the
      "requiresHostedExtensions" filter property. -->
   <extension id="vsphere.core.extensionHostFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.ExtensionHostFilter"/>
         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <name>requiresHostedExtensions</name>
               <type>String</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- User Settings filter-->
   <extension id="vsphere.core.userSettingsFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.UserSettingsFilter"/>
         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- 1. Expected that all the properties specified in the userSettingsConditions
                    are keys that can be retrieved using the persistence service.
                    2. The supported comparison operators are EQUALS, NOT_EQUALS and EQUALS_ANY_OF.
                    3. The supported conjoiner operators are AND, OR.
                    -->
               <name>userSettingsConditions</name>
               <type>com.vmware.data.query.CompositeConstraint</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Host Client filter -->
   <extension id="vsphere.core.hostClientFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.vsphere.client.filters.HostClientFilter"/>
         <returnsInstantly>true</returnsInstantly>
         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <name>hostClientRelevant</name>
               <type>Boolean</type>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Perspective Filter -->
   <extension id="vsphere.core.perspectiveTagFilter">
      <extendedPoint>vmware.extension.filters</extendedPoint>
      <object>
         <filterClass className="com.vmware.ui.perspective.PerspectiveTagFilter"/>
         <returnsInstantly>true</returnsInstantly>
         <filterProperties>
            <com.vmware.extensionfw.PropertyInfo>
               <!-- A list of perspective tags describing which perspectives the
                   extension can and cannot be included in. -->
               <name>tags</name>
               <!-- comma separated list of perspective tags -->
               <type>String</type>
               <optional>true</optional>
            </com.vmware.extensionfw.PropertyInfo>
         </filterProperties>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- IUID transformer -->
   <extension id="vsphere.core.UidTransformer">
      <extendedPoint>vmware.extension.transformers</extendedPoint>
      <object type="com.vmware.ui.uid.UidTransformer"/>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- GroupId transformer -->
   <extension id="vsphere.core.GroupIdTransformer">
      <extendedPoint>vmware.extension.transformers</extendedPoint>
      <object type="com.vmware.vsphere.client.groups.GroupIdTransformer"/>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- PrioritizedItem transformer -->
   <extension id="vsphere.core.prioritizedItemTransformer">
      <extendedPoint>vmware.extension.transformers</extendedPoint>
      <object type="com.vmware.vsphere.client.prioritization.PrioritizedItemTransformer"/>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!--
      Basic ActionCategory list. These are the generic ones we think most Actions
      will fit into; there are a couple of categories in the vm-ui plugin as well,
      and potentially any plugin could add its own but we hope third parties will
      avoid creating one-entry categories.
    -->
   <extension id="vsphere.core.powerOpsActionCategory">
      <extendedPoint>vmware.actions.categories</extendedPoint>
      <precedingExtension>vsphere.opsmgmt.alarms.alarmActionCategory</precedingExtension>
      <object>
         <label>#{actionCategories.PowerOps}</label>
      </object>
      <metadata>
         <tags>global, action</tags>
      </metadata>
   </extension>

   <extension id="vsphere.core.adminActionCategory">
      <extendedPoint>vmware.actions.categories</extendedPoint>
      <precedingExtension>vsphere.core.powerOpsActionCategory</precedingExtension>
      <object>
         <label>#{actionCategories.Administration}</label>
      </object>
      <metadata>
         <tags>global, action</tags>
      </metadata>
   </extension>

   <extension id="vsphere.core.configActionCategory">
      <extendedPoint>vmware.actions.categories</extendedPoint>
      <precedingExtension>vsphere.core.inventoryActionCategory</precedingExtension>
      <object>
         <label>#{actionCategories.Config}</label>
      </object>
      <metadata>
         <tags>global, action</tags>
      </metadata>
   </extension>

   <extension id="vsphere.core.inventoryActionCategory">
      <extendedPoint>vmware.actions.categories</extendedPoint>
      <precedingExtension>vsphere.core.configActionCategory</precedingExtension>
      <object>
         <label>#{actionCategories.Inventory}</label>
      </object>
      <metadata>
         <tags>global, action</tags>
      </metadata>
   </extension>

   <!-- The Action Framework will always display this special category last, so no
        need to define precedingExtension here -->
   <extension id="vsphere.core.miscellaneousActionCategory">
      <extendedPoint>vmware.actions.categories</extendedPoint>
      <object>
         <label>#{actionCategories.miscellaneous}</label>
      </object>
      <metadata>
         <tags>global, action</tags>
      </metadata>
   </extension>

   <!-- Action set for the resource framework's edit action -->
   <extension id="vsphere.core.resourcefw.inventoryActionSet">
      <extendedPoint>vise.actions.sets</extendedPoint>
      <object>
         <actions>
            <!-- The default edit dynamic resource action -->
            <com.vmware.actionsfw.ActionSpec>
               <!-- The uid matches ResourceViewEditResolver.DEFAULT_EDIT_ACTION_ID -->
               <uid>com.vmware.resourcefw.defaultEditAction</uid>
               <label>#{ResourceFw:editButton.label}</label>
               <delegate className="com.vmware.resourcefw.actions.ResourceViewEditResolver"/>
               <privateAction>true</privateAction>
            </com.vmware.actionsfw.ActionSpec>
         </actions>
      </object>
      <metadata>
         <tags>global, action</tags>
      </metadata>
   </extension>

   <!-- The control center view -->
   <extension id="vsphere.core.controlcenter.domainView">
      <extendedPoint>vise.global.views</extendedPoint>
      <precedingExtension>NULL</precedingExtension>
      <hostedPoint>vsphere.core.controlcenter.views</hostedPoint>
      <object>
         <name>#{controlCenter}</name>
         <icon>#{containerAppImages:home}</icon>
         <componentClass className="com.vmware.ui.views.TabbedView"/>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Control center - Getting Started tab -->
   <extension id="vsphere.core.controlcenter.gettingStartedHomeView">
      <extendedPoint>vsphere.core.controlcenter.views</extendedPoint>
      <object>
         <name>#{controlcenter.gettingStartedTab}</name>
         <componentClass>
            <className>com.vmware.vsphere.client.views.homeGS.GettingStartedHomeView</className>
            <object>
               <root>
                  <title>#{controlcenter.gettingStartedTab.title}</title>
                  <description>#{controlcenter.gettingStartedTab.description}</description>
                  <neverShowAgainKey>home.gs.neverShowAgain</neverShowAgainKey>
                  <descriptions>
                     <com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                        <image>#{containerAppImages:number1}</image>
                        <title>#{controlcenter.gettingStartedTab.description.objectNavigator.title}</title>
                        <description>#{controlcenter.gettingStartedTab.description.objectNavigator.description}</description>
                     </com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                     <com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                        <image>#{containerAppImages:number2}</image>
                        <title>#{controlcenter.gettingStartedTab.description.search.title}</title>
                        <description>#{controlcenter.gettingStartedTab.description.search.description}</description>
                     </com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                     <com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                        <image>#{containerAppImages:number3}</image>
                        <title>#{controlcenter.gettingStartedTab.description.information.title}</title>
                        <description>#{controlcenter.gettingStartedTab.description.information.description}</description>
                     </com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                     <com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                        <image>#{containerAppImages:number4}</image>
                        <title>#{controlcenter.gettingStartedTab.description.region.title}</title>
                        <description>#{controlcenter.gettingStartedTab.description.region.description}</description>
                     </com.vmware.vsphere.client.views.homeGS.GettingStartedTitledSectionInfo>
                  </descriptions>
                  <neverShowAgainKey>vsphere.core.controlcenter.gs.neverShowAgain</neverShowAgainKey>
               </root>
            </object>
         </componentClass>
      </object>
      <metadata>
         <requiresHostedExtensions>false</requiresHostedExtensions>
         <userSettingsConditions>
            <com.vmware.data.query.CompositeConstraint>
            <conjoiner>AND</conjoiner>
               <nestedConstraints>
                  <com.vmware.data.query.PropertyConstraint>
                     <propertyName>vsphere.core.controlcenter.gs.neverShowAgain</propertyName>
                     <comparator>NOT_EQUALS</comparator>
                     <comparableValue>
                        <Boolean>true</Boolean>
                     </comparableValue>
                  </com.vmware.data.query.PropertyConstraint>
                  <com.vmware.data.query.PropertyConstraint>
                     <propertyName>gettingStartedPages.neverShowAgain</propertyName>
                     <comparator>NOT_EQUALS</comparator>
                     <comparableValue>
                        <Boolean>true</Boolean>
                     </comparableValue>
                  </com.vmware.data.query.PropertyConstraint>
               </nestedConstraints>
            </com.vmware.data.query.CompositeConstraint>
         </userSettingsConditions>
         <tags>global, control</tags>
      </metadata>
   </extension>

   <!-- Control center - Application tab -->
   <extension id="vsphere.core.controlcenter.shortcutsView">
      <extendedPoint>vsphere.core.controlcenter.views</extendedPoint>
      <object>
         <name>#{controlcenter.applicationsTab}</name>
         <componentClass
            className="com.vmware.vsphere.client.views.controlcenter.ControlCenterView"/>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

