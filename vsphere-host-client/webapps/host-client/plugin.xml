         <label>#{controlcenter.category.administration}</label>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
   </extension>

   <!-- Control center - Legacy category -->
   <extension id="vsphere.core.controlcenter.legacyCategory">
      <extendedPoint>vsphere.core.controlcenter.shortcutCategories</extendedPoint>
      <object>
         <label>#{controlcenter.category.Legacy}</label>
      </object>
      <metadata>
         <tags>global</tags>
      </metadata>
