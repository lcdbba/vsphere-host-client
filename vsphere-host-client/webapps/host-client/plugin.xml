         <!-- Specialized instantiation strategies for "vise.actions.menus extensions"
              extension point -->
         <extensionPointId>vise.actions.menus</extensionPointId>
         <!-- The objects declared within "vise.actions.menus extensions"
              are created using the given constructor -->
